<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Students</title>
  </head>
  <body>
    <h1>Students</h1>
    <table border="1">
      <thead>
        <tr>
          <th>USN</th>
          <th>Name</th>
          <th>Gender</th>
          <th>Phone Number</th>
          <th>Email</th>
          <th>City</th>
        </tr>
      </thead>
      <tbody>
        <% students.forEach((student) => { %>
        <tr>
          <td><%= student.usn %></td>
          <td><%= student.name %></td>
          <td><%= student.gender %></td>
          <td><%= student.phoneNumber %></td>
          <td><%= student.email %></td>
          <td><%= student.city %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
